<div >
  <table class="table table-bordered table-striped">
    <caption>
      <h4>Panel Providers</h4>
    </caption>
    <tr>
      <th>Code</th>
      <th></th>
      <th>Evaluate Prices</th>
      <th></th>
      <!-- <th>edit</th>
      <th></th>
      <th>delete</th> -->
    </tr>
    <% if @panel_providers && @panel_providers.count == 0 %>
        <tr>
          <td colspan="7">There are no panel providers</td>
        </tr>
    <% else
         @panel_providers.each do |panel_provider| %>
            <tr>
              <td><%= panel_provider.code %></td>
              <td></td>
              <td><%= link_to "panel prices", {:controller => "panel_provider", :action => "evaluate_prices", :panel_provider_id => panel_provider.id} %></td>
              <td></td>
              <td><%#= link_to "edit", "panel_provider/edit" %> </td>
              <td></td>
              <td><%#= link_to "delete", "panel_provider/delete" %> </td>
            </tr>
        <% end
    end %>
    <tr>

    </tr>
    <tr>
      <td><%#= link_to "add new panel provider", "new" %> </td>
    </tr>
  </table>
</div>
